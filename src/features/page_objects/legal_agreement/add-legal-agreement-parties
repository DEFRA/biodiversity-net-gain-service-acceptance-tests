const Page = require("../page");

class LegalAgreementAddParties extends Page {
  get titleText() {
    return "Add all parties listed on the legal agreement";
  }

  get legalPartyName() {
    //Todo : return $("#organisation[0][organisationName]");
    return $(".govuk-input");
  }
    
  get legalPartyRole(){
    //Todo: fix when html on page is fixed and all ids are unique
    return $(".govuk-radios__input");
  }

  open() {
    return super.open("land/add-legal-agreement-parties");
  }
}
module.exports = new LegalAgreementAddParties();